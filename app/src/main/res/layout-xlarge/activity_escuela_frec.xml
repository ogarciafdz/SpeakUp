<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".EscuelaFrec" 
    android:background="@drawable/fondo" >


        <LinearLayout
            android1:id="@+id/linearLayout1"
            android1:layout_width="wrap_content"
            android1:layout_height="80dp"
            android1:orientation="horizontal" >

            <ImageButton
                android1:id="@+id/EFimageButton1"
                android1:layout_width="70dp"
                android1:layout_height="70dp"
                android1:src="@drawable/ic_lapiz" 
                android:contentDescription="@string/hello_world"/>

            <ImageButton
                android1:id="@+id/EFimageButton2"
                android1:layout_width="70dp"
                android1:layout_height="70dp"
                android1:src="@drawable/ic_tijeras" 
                android:contentDescription="@string/hello_world"/>

            <ImageButton
                android1:id="@+id/EFimageButton3"
                android1:layout_width="70dp"
                android1:layout_height="70dp"
                android1:src="@drawable/ic_sacpunt"
                android:contentDescription="@string/hello_world" />

            <ImageButton
                android1:id="@+id/EFimageButton4"
                android1:layout_width="75dp"
                android1:layout_height="75dp"
                android1:layout_marginBottom="51dp"
                android1:src="@drawable/ic_pegamento"
                android:contentDescription="@string/hello_world" />
        </LinearLayout>

        <LinearLayout
            android1:id="@+id/linearLayout2"
            android1:layout_width="wrap_content"
            android1:layout_height="wrap_content"
            android1:layout_alignRight="@+id/linearLayout1"
            android1:layout_below="@+id/linearLayout3"
            android1:layout_marginTop="28dp"
            android1:orientation="horizontal" >

            <ImageButton
                android1:id="@+id/EFimageButton5"
                android1:layout_width="65dp"
                android1:layout_height="65dp"
                android1:src="@drawable/ic_salir"
                android:contentDescription="@string/hello_world" />

            <ImageButton
                android1:id="@+id/EFimageButton6"
                android1:layout_width="77dp"
                android1:layout_height="77dp"
                android1:src="@drawable/ic_entrada"
                android:contentDescription="@string/hello_world" />

            <ImageButton
                android1:id="@+id/EFimageButton7"
                android1:layout_width="70dp"
                android1:layout_height="70dp"
                android1:src="@drawable/ic_bathroom" 
                android:contentDescription="@string/hello_world"/>

            <ImageButton
                android1:id="@+id/EFimageButton8"
                android1:layout_width="70dp"
                android1:layout_height="70dp"
                android1:src="@drawable/ic_sentarse" 
                android:contentDescription="@string/hello_world"/>
        </LinearLayout>

        <LinearLayout
            android1:id="@+id/linearLayout3"
            android1:layout_width="wrap_content"
            android1:layout_height="wrap_content"
            android1:layout_alignRight="@+id/linearLayout1"
            android1:layout_below="@+id/linearLayout1"
            android1:orientation="horizontal" >

            <ImageButton
                android1:id="@+id/EFimageButton9"
                android1:layout_width="70dp"
                android1:layout_height="70dp"
                android1:src="@drawable/ic_colorear"
                android:contentDescription="@string/hello_world" />

            <ImageButton
                android1:id="@+id/EFimageButton10"
                android1:layout_width="70dp"
                android1:layout_height="70dp"
                android1:src="@drawable/ic_cuaderno"
                android:contentDescription="@string/hello_world" />

            <ImageButton
                android1:id="@+id/EFimageButton11"
                android1:layout_width="70dp"
                android1:layout_height="70dp"
                android1:src="@drawable/ic_borrador" 
                android:contentDescription="@string/hello_world"/>

            <ImageButton
                android1:id="@+id/EFimageButton12"
                android1:layout_width="70dp"
                android1:layout_height="70dp"
                android1:src="@drawable/ic_lonche" 
                android:contentDescription="@string/hello_world"/>
        </LinearLayout>

</RelativeLayout>