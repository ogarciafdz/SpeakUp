        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageButton
                android:id="@+id/cine"
                android:layout_width="85dp"
                android:layout_height="80dp"
                android:contentDescription="@null"
